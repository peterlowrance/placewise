      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/src/assets/",
      "<rootDir>/src/environments/",
      "<rootDir>/src/test.ts",
      ".mock.service.ts",
      "<rootDir>/src/app/components/change-pass-dialog/",
